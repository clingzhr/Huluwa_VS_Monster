            <Image url="@../Background.png" />
         </image>
      </ImageView>
      <ImageView fx:id="startButton" fitHeight="55.0" fitWidth="500.0" layoutX="339.0" layoutY="282.0" onMouseClicked="#clickStartButton" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../startImage.png" />
         </image>
      </ImageView>
      <ImageView fx:id="historyButton" fitHeight="55.0" fitWidth="500.0" layoutX="339.0" layoutY="351.0" onMouseClicked="#clickHistoryButton" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../historyImage.png" />
         </image>
      </ImageView>
      <ImageView fx:id="aboutButton" fitHeight="55.0" fitWidth="500.0" layoutX="339.0" layoutY="416.0" onMouseClicked="#clickAboutButton" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../aboutImage.png" />
         </image>
      </ImageView>
      <ImageView fx:id="exitButton" fitHeight="55.0" fitWidth="500.0" layoutX="339.0" layoutY="483.0" onMouseClicked="#clickExitButton" pickOnBounds="true" preserveRatio="true">
